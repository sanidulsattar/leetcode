// Time Complexity: ğ‘‚(ğ‘›) | Space Complexity: ğ‘‚(1)

class Solution {
    public void sortColors(int[] nums) {
        int p=0;
        int temp =0;
        for(int i=0; i<nums.length; i++)
        {
            if(nums[p]==0)
            {
                p++;
                continue;
            }
            if(nums[i]==0)
            {
                temp=nums[p];
                nums[p]=nums[i];
                nums[i]=temp;
                p++;
            }
            
        }
         for(int i=p; i<nums.length; i++)
        {
            if(nums[p]==1)
            {
                p++;
                continue;
            }
            if(nums[i]==1)
            {
                temp=nums[p];
                nums[p]=nums[i];
                nums[i]=temp;
                p++;
            }
            
        }
        
    }
}
