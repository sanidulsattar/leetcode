class Solution {
    public int fib(int n) {
       int n1=1, n2=1, temp=0, sum=1;
       if(n>2){
       for(int i =3; i<=n; i++)
       {
           temp = n2;
           n2= n1+n2;
           n1 = temp;
         //  sum = sum+n2;
       }
       return n2;
       }
       else if(n==2 || n==1)
       {
           return 1;
       }
        
  return 0;  }
}
