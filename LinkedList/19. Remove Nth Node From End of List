// brute force technique with two passes
// Space and Time Complexity - O(n) and O(1)

class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        ListNode temp = head;
        int count =0;
        while(temp != null) {
            count++;
            temp = temp.next;
             }
            if(count== n)
            {
                return head.next;
            }       
        temp = head;
        for(int i = 0; i <count - n - 1; i++) {
            temp = temp.next;
             }
        temp.next = temp.next.next;
        return head;
    }
}
