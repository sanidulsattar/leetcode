// Time Complexity and Space Complexity : O(n) && O(1)

class Solution {
    public int removeDuplicates(int[] nums) {

        if(nums.length < 1)
        return 0;

        int low = 0;
        int k=1;
        for(int i=1; i < nums.length; i++ ){
            if(nums[low] != nums[i])
            {
                nums[++low] = nums[i]; 
                k++;
            }
        }
        return k;
        
    }
}
